<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="import-data" xsi:type="object">Railsformers\EcarImport\Console\Command\ImportData</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Framework\Config\File\ConfigFilePool">
        <arguments>
            <argument name="configFiles" xsi:type="array">
                <item name="system" xsi:type="string">Railsformers_EcarImport::system.xml</item>
            </argument>
        </arguments>
    </type>
</config>